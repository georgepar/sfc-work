tacker sfc-classifier-delete sfc1_http
tacker sfc-classifier-delete sfc1_ssh

tacker sfc-classifier-create --name sfc1_http --chain chainB --match source_port=0,dest_port=80,protocol=6
tacker sfc-classifier-create --name sfc1_ssh  --chain chainB --match source_port=0,dest_port=22,protocol=6
sleep 10
tacker sfc-classifier-list

compute_ip=10.20.0.5
ssh ${compute_ip} 'bash opnfv_demo/change_classifier.sh 17 5'
